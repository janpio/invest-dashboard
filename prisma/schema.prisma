generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "postgresql"
  url               = env("POSTGRES_PRISMA_URL")
  directUrl         = env("POSTGRES_URL_NON_POOLING")
  shadowDatabaseUrl = env("POSTGRES_URL_NON_POOLING")
}

model Operation {
  id            String      @id
  name          String
  date          DateTime
  type          Int
  figi          String
  payment       Int
  commission    Int
  quantity      Int
}

model Intrument {
  figi          String      @id
  ticker        String
  lot           Int
  currency      String
  name          String
}
